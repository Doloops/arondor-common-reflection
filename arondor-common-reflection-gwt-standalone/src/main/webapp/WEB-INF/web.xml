<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>

	<servlet>
		<servlet-name>reflectionService</servlet-name>
		<servlet-class>com.arondor.common.reflection.gwt.server.DefaultGWTReflectionService</servlet-class>
	</servlet>


	<servlet>
		<servlet-name>objectConfigurationService</servlet-name>
		<servlet-class>com.arondor.common.reflection.gwt.server.DefaultGWTObjectConfigurationService</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>reflectionService</servlet-name>
		<url-pattern>/ArondorReflectionGWT/reflectionService</url-pattern>
	</servlet-mapping>
	
	
	<servlet-mapping>
		<servlet-name>objectConfigurationService</servlet-name>
		<url-pattern>/ArondorReflectionGWT/objectConfigurationService</url-pattern>
	</servlet-mapping>
	

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>gwtClassDesigner.html</welcome-file>
	</welcome-file-list>
</web-app> 
