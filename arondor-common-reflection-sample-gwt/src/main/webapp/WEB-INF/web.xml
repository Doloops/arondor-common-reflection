<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>

	<servlet>
		<servlet-name>configurationService</servlet-name>
		<servlet-class>com.arondor.common.reflection.sample.gwt.server.SampleConfigurationService</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>configurationService</servlet-name>
		<url-pattern>/ArondorReflectionSampleGWT/configurationService</url-pattern>
	</servlet-mapping>

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>reflectionGwtSample.html</welcome-file>
	</welcome-file-list>
</web-app> 
